{
  "rules": {
    "Check": {
      ".write": "auth != null",
      ".read": "auth != null",
      ".indexOn": [
        "Status",
        "NumeroSerie"
      ]
    },
    "Autos": {
      ".write": "auth != null",
      ".read": "auth != null"
    },
    "CambiosCheck": {
      ".write": "auth != null",
      ".read": "auth != null"
    },
      "Ventas": {
      ".write": "auth != null",
      ".read": "auth != null"
    },
      "Clientes": {
      ".write": "auth != null",
      ".read": "auth != null"
    },
    "usuarios": {
      "$uid": {
        ".read": "$uid === auth.uid"
      }
    },
      "Documentos": {
        ".write": "root.child('/usuarios/' + auth.uid + '/Permisos/').val() == 'administrador'",
        ".read": "root.child('/usuarios/' + auth.uid + '/Permisos/').val() == 'administrador'"
      }
  }
}